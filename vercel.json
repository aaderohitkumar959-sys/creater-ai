{
    "version": 2,
    "builds": [
        {
            "src": "apps/web/package.json",
            "use": "@vercel/next",
            "config": {
                "buildCommand": "npm run vercel-build",
                "outputDirectory": "apps/web/.next"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/apps/web/$1"
        }
    ]
}